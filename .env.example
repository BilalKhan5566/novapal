# Google Custom Search API
GOOGLE_CSE_API_KEY=your_google_cse_api_key_here
GOOGLE_CSE_CX=your_google_cse_cx_here

# Google Gemini API
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# OpenAI API (alternative to Gemini)
OPENAI_API_KEY=your_openai_api_key_here

# LLM Provider: 'gemini' or 'openai' (defaults to 'openai')
LLM_PROVIDER=openai

# Database (Turso)
TURSO_CONNECTION_URL=your_turso_connection_url_here
TURSO_AUTH_TOKEN=your_turso_auth_token_here